        });
        var getScrollTop = function() {
            return Math.max(window.pageYOffset, document.documentElement.scrollTop, document.body.scrollTop);
        };
        var $_snapshot = document.querySelector('section.snapshot');
        var $_header = document.querySelector('header.header');
        var $_search = document.querySelector('div.search__bar');
        function updateScroll() {
            var scrollTop = getScrollTop();
            if (scrollTop > 0) {
                document.body.classList.add('scrolled');
            } else {
                document.body.classList.remove('scrolled');
            }
//            if ($_snapshot) {
//                var $_parent = $_snapshot.parentElement;
//                var headerRect = $_header.getBoundingClientRect();
//                var searchRect = $_search.getBoundingClientRect();
//                if (scrollTop > searchRect.height - headerRect.height + 1) {
//                    $_parent.classList.add('snapshot--fixed');
//                    var snapshotRect = $_snapshot.getBoundingClientRect();
//                    $_parent.style.paddingTop = headerRect.height + snapshotRect.height + 'px';
//                    $_snapshot.style.top = headerRect.height + 'px';
//                } else {
//                    $_parent.classList.remove('snapshot--fixed');
//                    $_parent.style.removeProperty('padding-top');
//                }
//            }
        }
        function monitorWindow() {
            if (!ticking) {
                window.requestAnimationFrame(function() {
                    updateScroll();
                    ticking = false;
                });
                ticking = true;
            }
        }
